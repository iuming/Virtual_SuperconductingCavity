record(ai, "Ts") {
    field(DESC, "Sampling Time")
    field(EGU, "s")
    field(PINI, "YES")
    field(VAL, "1e-7")
}

record(ai, "Ig_I") {
    field(DESC, "I component of the current of the generator")
    field(EGU, "A")
    field(PINI, "YES")
    field(VAL, "32e-3")
}

record(ai, "Ig_Q") {
    field(DESC, "Q component of the current of the generator")
