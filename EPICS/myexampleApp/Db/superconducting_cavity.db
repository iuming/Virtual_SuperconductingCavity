record(calc, "CurrentTime") {
	field(DESC, "simulate current time")
	field(EGU, "us")
    field(CALC, "A+1")
    field(INPA, "CurrentTime PP")
    field(SCAN,".1 second")
}


record(ai, "V_I") {
    field(DESC, "I component of the cavity voltage")
    field(EGU, "V")
