}

record(ai, "dOmega1_dt") {
    field(DESC, "The first mode of mechanicial detuning partical")
    field(EGU, "rad/s")
    field(PINI, "YES")
    field(VAL, "0")
}

record(ai, "dOmega2_dt") {
    field(DESC, "The second mode of mechanicial detuning partical")
    field(EGU, "rad/s")
